#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = erpServer.exe erpServerClient.exe 
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

erpServer.exe: erpServer.dpr
  $(DCC)

erpServerClient.exe: erpServerClient.dpr
  $(DCC)
